{
    "$schema": "http://json-schema.org/schema",
    "$id": "realdolmen",
    "title": "Super UI library Component schema",
    "type": "object",
    "properties": {
      "app":{
        "description": "The name of the application to generate",
        "type":"string",
        "enum": [
            "predefined","defaultApp"
          ],
          "default":"defaultApp",
    
          "x-prompt":{
            "message":"What type of application would you like to add",
            "type":"list",
            "uniqueItems": true,
          "minItems": 1,
            "list":[
              {"value":"predefined","label":"predefined"},
              {"value":"defaultapp","label":"defaultApp"}
          
              
            ]
          }
        }
    },
        
    "required": [
      "app"
    ]
  
}