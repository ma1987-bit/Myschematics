{
    "$schema": "http://json-schema.org/schema",
    "$id": "realdolmen",
    "title": "Super UI library Component schema",
    "type": "object",
    "properties": {
        "name": {
            "description": "The name of the model.",
            "type": "string",
            "x-prompt":"Can you please add the model name ?"
          },
      "app":{
        "description": "The name of the application to generate",
        "type":"string",
        "enum": [
            "fakestore","Shop-App"
          ],
          "default":"header",
    
          "x-prompt":{
            "message":"What type of application would you like to add",
            "type":"list",
            "uniqueItems": true,
          "minItems": 1,
            "list":[
              {"value":"api-application","label":"Api-App"},
              {"value":"shop","label":"Shop-App"}
              
            ]
          }
        }
    },
        
    "required": [
      "app"
    ]
  
}